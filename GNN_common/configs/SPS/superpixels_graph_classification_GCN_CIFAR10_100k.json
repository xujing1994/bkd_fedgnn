{
    "gpu": {
        "use": true,
        "id": 1
    },
    
    "model": "GCN",
    "dataset": "CIFAR10",
    
    "out_dir": "out/superpixels_graph_classification/",
    
    "params": {
        "seed": 41,
        "epochs": 5000,
        "batch_size": 512,
        "init_lr": 0.001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-5,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 146,
        "out_dim": 146,
        "residual": true,
        "readout": "mean",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true,
        "self_loop": false
    }
}
