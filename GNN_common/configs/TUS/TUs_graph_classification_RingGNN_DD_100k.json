{
    "gpu": {
        "use": true,
        "id": 1
    },
    
    "model": "RingGNN",
    "dataset": "DD",
    
    "out_dir": "out/TUs_graph_classification/",

    "params": {
        "seed": 41,
        "epochs": 1000,
        "train_size": 110,
        "val_size": 110,
        "test_size": 110,
        "batch_size": 4,
        "init_lr": 1e-4,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 2000,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 2,
        "hidden_dim": 20,
        "radius": 2,
        "residual": false,
        "dropout": 0.0,
        "layer_norm": false
    }
}