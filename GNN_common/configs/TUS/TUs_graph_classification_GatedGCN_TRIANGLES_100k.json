{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "GatedGCN",
    "dataset": "TRIANGLES",
    
    "out_dir": "results/TUs_graph_classification/",
    
    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 20,
        "init_lr": 7e-4,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 25,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 3,
        "hidden_dim": 64,
        "out_dim": 64,
        "residual": true,
        "readout": "sum",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true,
        "edge_feat": false
    }
}
