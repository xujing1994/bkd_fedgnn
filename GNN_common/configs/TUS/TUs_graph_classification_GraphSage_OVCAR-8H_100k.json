{
    "gpu": {
        "use": true,
        "id": 1
    },
    
    "model": "GraphSage",
    "dataset": "OVCAR-8H",
    
    "out_dir": "out/TUs_graph_classification/",
    
    "params": {
        "seed": 41,
        "epochs": 10000,
        "batch_size": 20,
        "train_size": 100,
        "val_size": 100,
        "test_size": 100,
        "init_lr": 0.001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 2000,
        "min_lr": 1e-6,
        "weight_decay": 8e-4,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 90,
        "out_dim": 90,
        "residual": true,
        "readout": "mean",
        "in_feat_dropout": 0.0,
        "dropout": 0.2,
        "batch_norm": true,
        "sage_aggregator": "mean"
    }
}
