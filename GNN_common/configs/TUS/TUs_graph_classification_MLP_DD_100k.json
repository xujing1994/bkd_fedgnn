{
    "gpu": {
        "use": true,
        "id": 1
    },
    
    "model": "MLP",
    "dataset": "DD",
    
    "out_dir": "results/TUs_graph_classification/",

    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 20,
        "train_size": 110,
        "val_size": 110,
        "test_size": 110,
        "init_lr": 1e-4,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 2000,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 154,
        "out_dim": 154,
        "readout": "mean",
        "gated": false,
        "in_feat_dropout": 0.0,
        "dropout": 0.0
    }
}